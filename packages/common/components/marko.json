{
  "taglib-imports": [
    "./elements/marko.json",
    "./blocks/marko.json",
    "./dpm/marko.json"
  ],
  "<common-document>": {
    "template": "./document.marko",
    "<head>": {},
    "@newsletter": "object",
    "@date": "date"
  },
  "<common-static-styles>": {
    "template": "./static-styles.marko"
  },
  "<common-table>": {
    "template": "./core-table.marko",
    "@border": "number",
    "@width": "string",
    "@class": "string",
    "@spacing": "number",
    "@padding": "number",
    "@align": "string",
    "@style": "string",
    "@attrs": "object"
  },
  "<native-x-fetch>": {
    "template": "./native-x-fetch.marko",
    "@uri": "string",
    "@placement-id": "string",
    "@date": "date",
    "@image-options": "object",
    "@logo-options": "object"
  }
}
